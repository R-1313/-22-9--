#include <iostream>
using namespace std;

int main() {
    int a = 7;
    int b = 2;
    int c = 4;
    int sum = 0;
    int summint;
    cout <<"Вводим кол-во минут ";
    cin >> summint;
    if (summint < a) {
        sum = summint * b;
        cout << sum <<endl;
    } else {
        sum = summint * c;
        cout << sum <<endl;
    }
}
